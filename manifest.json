{
  "manifest_version": 3,

  "name": "IhsanShip for app2",
  "description": "Let automation take care of shipping.",
  "version": "1.0.0",
  "icons": { "128": "icon_128.png" },
  "action": {
    "default_icon": "icon.png",
    "default_popup": "popup.html"
  },
  "permissions": ["activeTab"],
  "content_scripts": [{
    "matches": ["https://app2.dhlexpresscommerce.com/orders/new/*"],
    "js": ["Content.js"]
    }
  ]
}
